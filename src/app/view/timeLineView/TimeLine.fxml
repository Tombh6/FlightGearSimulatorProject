<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="74.0" prefWidth="1220.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.view.timeLineView.TimeLineController">

            <Slider fx:id="time" layoutX="14.0" layoutY="30.0" prefHeight="14.0" prefWidth="893.0" />
            <HBox layoutX="923.0" layoutY="24.0" spacing="10.0">
               <children>
                  <Button fx:id="back" mnemonicParsing="false" text="-15s" />
                  <Button fx:id="stop" mnemonicParsing="false" text="Stop" />
                  <Button fx:id="play" mnemonicParsing="false" text="Start" />
                  <Button fx:id="pause" mnemonicParsing="false" text="Pause" />
                  <Button fx:id="next" mnemonicParsing="false" text="+15s" />
                  <HBox>
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Operating speed" />
                        <ComboBox fx:id="speed" prefHeight="30.0" prefWidth="80" />
                     </children>
                  </HBox>
               </children>
            </HBox>
            <Text fx:id="textTimeStamp" layoutX="402.0" layoutY="18.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Timeline" underline="true">
      <font>
         <Font size="14.0" />
      </font></Text>
      </AnchorPane>
